# A simple Fibonacci function
function fib(n)
    # 0, 1
    if n < 2
        # return n
        return n
    else
        # Otherwise, return fib(n - 1) + fib(n - 2)
        return fib(n - 1) + fib(n - 2)
    end
end

# The main entry point
function main()
    # assign x to be 10
    let x = 10

    # Print a message and the fib(x) value
    print "Fibonacci of" x "is" fib(x)
end
