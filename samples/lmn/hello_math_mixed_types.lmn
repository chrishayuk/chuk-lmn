function main()
  // a => i32
  set a 10
  
  // big => i64 (exceeds 2^31-1)
  set big 10000000000

  // f => f32 (decimal literal)
  set f 3.14

  // sum => a + f => i32 + f32 => f32
  set sum a + f
  print sum

  // x => i32
  set x 42

  // y => f32 (decimal literal)
  set y 2.5

  // product => x * y => i32 * f32 => f32
  set product x * y
  print product
end
